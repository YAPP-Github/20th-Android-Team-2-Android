<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">

    <!-- regular -->
    <font
        android:font="@font/spoqa_han_sans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/spoqa_han_sans_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- medium -->
    <font
        android:font="@font/spoqa_han_sans_medium"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/spoqa_han_sans_medium"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- bold -->
    <font
        android:font="@font/spoqa_han_sans_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/spoqa_han_sans_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>
